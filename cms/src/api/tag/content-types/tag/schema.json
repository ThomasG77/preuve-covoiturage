{
  "kind": "collectionType",
  "collectionName": "tags",
  "info": {
    "singularName": "tag",
    "pluralName": "tags",
    "displayName": "Tag"
  },
  "options": {
    "draftAndPublish": false
  },
  "pluginOptions": {},
  "attributes": {
    "label": {
      "type": "string",
      "unique": false,
      "required": true
    },
    "slug": {
      "type": "string",
      "regex": "^([a-zA-Z-]*)$",
      "unique": true,
      "required": true
    },
    "pages": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::page.page",
      "mappedBy": "tags"
    }
  }
}

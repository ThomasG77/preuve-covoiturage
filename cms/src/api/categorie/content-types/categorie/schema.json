{
  "kind": "collectionType",
  "collectionName": "categories",
  "info": {
    "singularName": "categorie",
    "pluralName": "categories",
    "displayName": "Categorie",
    "description": ""
  },
  "options": {
    "draftAndPublish": false
  },
  "pluginOptions": {},
  "attributes": {
    "label": {
      "type": "string",
      "required": true
    },
    "slug": {
      "type": "string",
      "required": true,
      "regex": "^([a-zA-Z-]*)$"
    },
    "articles": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::article.article",
      "mappedBy": "categorie"
    }
  }
}
